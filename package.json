    "lint:check": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs --ignore-path .gitignore",
    "format:write": "prettier --write src/",
    "format:check": "prettier --check src/"
  },
  "dependencies": {
    "vue": "^3.4.29",
    "vue-router": "^4.3.3",
    "bootstrap": "^5.3.3"
  },
  "devDependencies": {
