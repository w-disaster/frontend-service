{
  "name": "template-for-vuejs-projects",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs",
